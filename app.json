{
  "expo": {
    "name": "GuardManage",
    "slug": "expoStack",
    "version": "1.0.0",
    "scheme": "guardmanage",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Guard Manage to use your location."
        }
      ],
      [
        "expo-font",
        {
          "fonts":["./assets/fonts/Nunito-Bold.ttf","./assets/fonts/Nunito-Regular.ttf","./assets/fonts/Nunito-SemiBold.ttf"]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#291D89"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#291D89"
      },
      "package": "com.guardmanage.app"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "370ea780-722d-419e-8bca-3c568096b202"
      }
    }
  }
}
